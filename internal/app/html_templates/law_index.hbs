<div class="container">
    <h1>{{ Title }}</h1>

    <div class="col">
        <div class="btn-group" role="group">
            {{> 'partials/bar-button-item' Label="All Laws"     T1=""       T2=LawType }}
        </div>
        <div class="btn-group" role="group">
            {{> 'partials/bar-button-item' Label="House Bills"  T1="H.R."   T2=LawType}}
            {{> 'partials/bar-button-item' Label="House Joint Resolutions"  T1="H.J."   T2=LawType}}
        </div>

        <div class="btn-group" role="group">
            {{> 'partials/bar-button-item' Label="Senate Bills" T1="S."     T2=LawType}}
            {{> 'partials/bar-button-item' Label="Senate Joint Resolutions" T1="S.J."     T2=LawType}}
            {{> 'partials/bar-button-item' Label="Senate Resolutions" T1="S.Res." T2=LawType}}
        </div>

        <div class="btn-group" role="group">
            {{> 'partials/bar-button-item' Label="Public Laws"  T1="Public" T2=LawType}}
        </div>
    </div>

    {{> 'partials/law-list' EnableLoad=true }}
</div>
